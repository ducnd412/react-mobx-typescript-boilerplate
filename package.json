    "redbox-react": "^1.3.2",
    "roadhog": "^1.3.1",
    "roadhog-api-doc": "^0.3.3",
    "rollbar": "^2.3.1",
    "stylelint": "^8.1.0",
    "stylelint-config-standard": "^17.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "mobx-react-router": "^4.0.1",
    "numeral": "^2.0.6",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0",
    "antd": "^3.0.0-beta.1",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.9.2",
    "core-js": "^2.5.1",
    "dva": "^2.1.0",
    "g-cloud": "^1.0.2-beta",
    "g2": "^2.3.13",
    "g2-plugin-slider": "^1.2.1",
    "lodash": "^4.17.4",
    "lodash-decorators": "^4.4.1",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.19.1",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "react-container-query": "^0.9.1",
    "react-document-title": "^2.0.3",
    "react-fittext": "^1.0.0"
  },
  "optionalDependencies": {
    "nightmare": "^2.10.0"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setupTests.js"
    ],
    "testMatch": [
      "**/?(*.)(spec|test|e2e).js?(x)"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/jasmine.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/tests/styleMock.js"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
